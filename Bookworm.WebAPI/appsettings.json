{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cache": {
    "Provider": "Memory"
  },
  "ConnectionStrings": {
    "CacheDbConnection": "Server=myServerAddress;Database=myCacheDb;User Id=myUsername;Password=myPassword;",
    "RedisConnection": "localhost:6379"
  },
  "AllowedHosts": "*"
}

